# Unigram
U00:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
U04:%x[2,0]
U05:%x[-2,0]/%x[-1,0]
U06:%x[-1,0]/%x[0,0]
U07:%x[-1,0]/%x[1,0]
U08:%x[0,0]/%x[1,0]
U09:%x[1,0]/%x[2,0]
U10:%x[0,0]/%x[0,2]
U11:%x[0,0]/%x[0,3]
U12:%x[0,0]/%x[-1,2]/%x[0,2]
U13:%x[0,0]/%x[0,2]/%x[1,2]

# Bigram
B00:%x[-2,0]
B01:%x[-1,0]
B02:%x[0,0]
B03:%x[1,0]
B04:%x[2,0]
B05:%x[-1,0]/%x[0,0]
B06:%x[-1,0]/%x[1,0]
B07:%x[0,0]/%x[1,0]
B08:%x[0,0]/%x[0,2]
B09:%x[0,0]/%x[0,3]
